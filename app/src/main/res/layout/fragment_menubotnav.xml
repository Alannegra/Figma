<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".Menubotnav">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="350dp"
            android:layout_height="wrap_content"
            android:scaleX="2.4"

            android:src="@android:drawable/bottom_bar"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintLeft_toRightOf="parent"

            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <ImageView
            android:id="@+id/map"
            android:layout_width="50dp"
            android:layout_height="50dp"


            android:src="@drawable/mapawhite"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/carmarketbutton"
            app:layout_constraintStart_toStartOf="parent" />

        <ImageView
            android:id="@+id/carmarketbutton"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:src="@drawable/shoping"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/earthbutton"
            app:layout_constraintStart_toEndOf="@+id/map" />

        <ImageView
            android:id="@+id/earthbutton"
            android:layout_width="45dp"
            android:layout_height="40dp"
            android:layout_marginBottom="8dp"


            android:src="@drawable/earthicon"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/carmarketbutton" />


    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>